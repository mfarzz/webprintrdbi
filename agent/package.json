{
  "name": "agent",
  "version": "1.0.0",
  "description": "A small Node.js agent app",
  "main": "agent.js",
  "bin": "agent.js",
  "scripts": {
  "start": "node agent.js",
  "build": "pkg . --targets node18-win-x64 --output agent.exe --no-bytecode --public-packages '*' --no-warnings --no-windows-console"
},
  "pkg": {
    "assets": [
      "SumatraPDF.exe",
      "SumatraPDF-settings.txt",
      ".env"
    ],
    "options": {
      "no-windows-console": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^11.3.2"
  }
}
